<!----- JQUERY ----->
 
<script>

$(document).ready(function(){
    
    /// PHOTOSETS 
    
    /// pxu photosets by pixelunion
    
   $('.photo-slideshow').pxuPhotoset({
       lightbox: true,
       rounded: false,
       gutter: '{select:photoset gutter}px',
       photoset: '.photo-slideshow',
       photoWrap: '.photo-data',
       photo: '.pxu-photo'
   });
   
   /// npfPhotosets by @codematurgy
   
    npfPhotosets(".posts", {
       rowClass:"npf_row",
       imageContainerClass:"tmblr-full",
       generatedPhotosetContainerClass:"npf_photoset",
       imageClass:"npf_image",
       includeCommonPhotosets: false,
       useTumblrLightbox: true,
       insertGalleryIndicator: false,
       galleryIndicatorClass: "npf_gallery_indicator",
       galleryIndicatorContent: "<img src='image_url'>",
       photosetMargins:"{select:photoset gutter}"
    });
    
    /// NOTECOUNT by @shythemes
    
    $('article').find('.notecount').each(function(){
       var n = $(this).html().split(' ')[0].replace(/,/g, '');
       if (n > 999) {
           n = Math.floor(n / 100) / 10;
           $(this).text(n + 'k notes');
       }
   });
   
   /// TOOLTIPS by atomiks @ atomiks.github.io

    tippy('[title]', {
        theme: 'custom',
        arrow: false,
        followCursor: true,
        delay: 100,
        placement: 'bottom-start',
        zIndex: 9999999999,
        maxWidth: 400,
    
        content(reference) {
          const title = reference.getAttribute('title');
          reference.removeAttribute('title');
          return title;
        },
    });
    
    /// TAGS ON CLICK AND INFO DROPDOWN

    {block:IndexPage}
    
    {block:ifTagsOnClick}
    $('.tag-click').hide();
    $('.tag-button').click(function(event) {
        $(this).closest('.posts').find('.tag-click').slideToggle(500)
        event.preventDefault();
        $(this).toggleClass('fill', 500);
    });
    {/block:ifTagsOnClick}
    
    {block:ifPostInfoDropdown}
    $('.info-dropdown').hide();
    $('.info-button').click(function(event) {
        $(this).closest('.posts').find('.info-dropdown').slideToggle(500)
        event.preventDefault();
        $(this).toggleClass('info-clicked', 500);
    });
    {/block:ifPostInfoDropdown}
    
    var clipboard = new ClipboardJS(".copy-button");
    
    $('.copy-message').hide();
    $('.copy-button').click(function(event) {
        $(this).closest('.posts').find('.copy-message').fadeIn(500)
        event.preventDefault();
        
        setTimeout(function(){
            $('.copy-message').fadeOut(500);}, 2000);
    });
    
    {/block:IndexPage}
    
    /// TIME AGO by @bychloethemes
    
    $(".time-ago").timeAgo({
        time: "letter",
        spaces: true, 
        words: false, 
        prefix: "", 
        suffix: "ago", 
    });
    
    /// REDIRECTS by @magnusthemes
    
    $('a[href*="t.umblr.com/redirect"]').each(function(){
      var originalURL = $(this).attr("href").split("?z=")[1].split("&t=")[0];
      var replaceURL = decodeURIComponent(originalURL);
      $(this).attr("href", replaceURL);
  });
  
    /// SLIDE MOBILE NAV
    
    $('.menu-button').click(function(){
        $('#sidebar').addClass('show');
    });
   
    $('.close').click(function(){
        $('#sidebar').removeClass('show');
    });
    
    /// SCROLL TO TOP
    
     $('#btop').click(function(){
        $('html, body').animate({scrollTop : 0},800);
        return false;
    });
    
});

/// RESPONSIVE VIDEOS

    function flexFrame() {
   $(".caption").each(function() {
        $(this).find("iframe").wrap("<div class='capframe'></div>"); 
        flexibleFrames($(".capframe"));
    });
    flexibleFrames($(".video"));
}
$(document).ready(flexFrame);

feather.replace()

</script> 


